{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#DEE2E6",
					"name": "Light Gray",
					"slug": "light-gray"
				},
				{
					"color": "#495057",
					"name": "Dark Gray",
					"slug": "dark-gray"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				}
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"textColumns": true,
			"writingMode": true,
			"fontFamilies": [
				{
					"name": "System",
					"slug": "system",
					"fontFamily": "system-ui, sans-serif"
				}
			],
			"fontSizes": [
				{
					"name": "Extra Small",
					"slug": "x-small",
					"size": ".95rem",
					"fluid": {
						"min": ".825rem",
						"max": ".95rem"
					}
				},
				{
					"name": "Small",
					"slug": "small",
					"size": "1.05rem",
					"fluid": {
						"min": ".9rem",
						"max": "1.05rem"
					}
				},
				{
					"name": "Base",
					"slug": "base",
					"size": "1.165rem",
					"fluid": {
						"min": "1rem",
						"max": "1.165rem"
					}
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "1.65rem",
					"fluid": {
						"min": "1.2rem",
						"max": "1.65rem"
					}
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "2.75rem",
					"fluid": {
						"min": "1.5rem",
						"max": "2.75rem"
					}
				},
				{
					"name": "Extra Large",
					"slug": "x-large",
					"size": "3.5rem",
					"fluid": {
						"min": "1.875rem",
						"max": "3.5rem"
					}
				},
				{
					"name": "Extra Extra Large",
					"slug": "xx-large",
					"size": "4.3875rem",
					"fluid": {
						"min": "2.25rem",
						"max": "4.3875rem"
					}
				}
			]
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"blockGap": true,
			"padding": true,
			"margin": true,
			"spacingSizes": [
				{
					"name": "Small",
					"size": "clamp(.5rem, 2.5vw, 1rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.5rem, 4vw, 2rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(2rem, 5vw, 3rem)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(3rem, 7vw, 5rem)",
					"slug": "x-large"
				},
				{
					"name": "2xl",
					"size": "clamp(4rem, 9vw, 7rem)",
					"slug": "xx-large"
				},
				{
					"name": "3xl",
					"size": "clamp(5rem, 12vw, 9rem)",
					"slug": "xxx-large"
				},
				{
					"name": "4xl",
					"size": "clamp(6rem, 14vw, 13rem)",
					"slug": "xxxx-large"
				}
			],
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"layout": {
			"contentSize": "645px",
			"wideSize": "1340px"
		},
		"custom": {
			"defaults": {
				"transition-duration": "0.25s"
			},
			"typography": {
				"lineHeight": {
					"none": 1,
					"xx-small": 1.125,
					"x-small": 1.25,
					"small": 1.375,
					"medium": 1.5,
					"large": 1.675,
					"x-large": 1.75,
					"xx-large": 2
				},
				"fontWeight": {
					"thin": 100,
					"extra-light": 200,
					"light": 300,
					"regular": 400,
					"medium": 500,
					"semi-bold": 600,
					"bold": 700,
					"extra-bold": 800,
					"black": 900
				}
			},
			"border": {
				"radius": {
					"x-small": "0.125rem",
					"small": "0.25rem",
					"medium": "0.375rem",
					"large": "0.5rem",
					"x-large": "0.625rem",
					"full": "100vmax"
				}
			}
		}
	},
	"styles": {
		"color": {
			"text": "var:preset|color|black",
			"background": "var:preset|color|white"
		},
		"typography": {
			"fontFamily": "var:preset|font-family|system",
			"fontSize": "var:preset|font-size|base",
			"fontWeight": "var:custom|typography|font-weight|regular",
			"lineHeight": "var:custom|typography|line-height|x-large"
		},
		"spacing": {
			"blockGap": "var:preset|spacing|medium",
			"padding": {
				"right": "var:preset|spacing|medium",
				"left": "var:preset|spacing|medium"
			}
		},
		"elements": {
			"heading": {
				"typography": {
					"fontWeight": "var:custom|typography|font-weight|bold",
					"lineHeight": "var:custom|typography|line-height|xx-small"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|x-large"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|large"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"lineHeight": "var:custom|typography|line-height|x-small"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|base",
					"lineHeight": "var:custom|typography|line-height|x-small"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"lineHeight": "var:custom|typography|line-height|x-small"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|x-small",
					"lineHeight": "var:custom|typography|line-height|x-small"
				}
			},
			"link": {
				"css": "& { transition: text-decoration-color 0.2s ease-out; } &:hover { text-decoration-color: transparent }"
			},
			"button": {
				"color": {
					"text": "var:preset|color|white",
					"background": "var:preset|color|black"
				},
				"typography": {
					"lineHeight": "var:custom|typography|line-height|x-small",
					"textDecoration": "underline"
				},
				"spacing": {
					"padding": {
						"top": ".75em",
						"right": "1.125em",
						"bottom": ".75em",
						"left": "1.125em"
					}
				},
				"border": {
					"style": "solid",
					"width": "1px",
					"color": "var:preset|color|black",
					"radius": "var:custom|border|radius|medium"
				},
				":hover": {
					"color": {
						"background": "var:preset|color|dark-gray"
					},
					"border": {
						"color": "var:preset|color|dark-gray"
					}
				},
				":focus": {
					"color": {
						"background": "var:preset|color|dark-gray"
					},
					"border": {
						"color": "var:preset|color|dark-gray"
					},
					"outline": {
						"offset": "3px"
					}
				},
				"css": "& { transition: all 0.2s ease-out; text-decoration-color: transparent; } &:where(:hover, :focus, :focus-visible) { text-decoration-color: currentColor; }"
			}
		},
		"blocks": {
			"core/accordion": {
				"spacing": {
					"blockGap": "var:preset|spacing|small"
				}
			},
			"core/accordion-item": {
				"color": {
					"background": "var:preset|color|white"
				},
				"spacing": {
					"blockGap": "0"
				},
				"border": {
					"style": "solid",
					"width": "1px",
					"color": "var:preset|color|light-gray",
					"radius": "var:custom|border|radius|medium"
				}
			},
			"core/accordion-heading": {
				"typography": {
					"fontSize": "inherit",
					"fontWeight": "var:custom|typography|font-weight|semi-bold"
				},
				"css": "&__toggle { padding: var(--wp--preset--spacing--small); } &__toggle-icon { font-size: 110%; height: 0.8em; width: 0.8em; }"
			},
			"core/accordion-panel":{
				"spacing": {
					"padding": {
						"right": "var:preset|spacing|small",
						"bottom": "var:preset|spacing|small",
						"left": "var:preset|spacing|small"
					}
				}
			}
		},
		"css": ":root :where(.is-layout-flow) > :is(h1, h2, h3, h4, h5, h6) { margin-block-start: var(--wp--preset--spacing--large); }"
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "sidebar",
			"name": "sidebar",
			"title": "Sidebar"
		}
	],
	"customTemplates": [
		{
			"name": "page-no-title",
			"postTypes": ["page"],
			"title": "Page No Title"
		}
	]
}